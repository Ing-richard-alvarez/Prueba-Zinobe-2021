<!DOCTYPE html>
<html lang="en">
<head>
    {% block head %}
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
    {% endblock head %}
    
</head>
<body>

    {% block menuNav %}
        {{ include('template-parts/header.twig') }}
    {% endblock menuNav %}

    <div class="container">
        {% block content %}{% endblock content %}
    </div>

    {% block modalsWindow %}{% endblock modalsWindow %}
    
    {% block handlebarsTemplate %}{% endblock handlebarsTemplate %}
    
    <footer>
        {% block footer %}{% endblock footer %}    
    </footer>
    
    {% block scripts %}{% endblock scripts %}

</body>
</html>